{
    "files.associations": {
        "LICENSE": "plaintext"
    },
    "[markdown][plaintext]": {
        "editor.rulers": [80],
        "editor.wordWrap": "off",
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.formatOnSave": true
    },
    "[python]": {
        "editor.rulers": [120],
        "editor.defaultFormatter": "ms-python.autopep8",
        "editor.formatOnSave": true
    },
    "cSpell.words": [
        "choosealicense",
        "Ojos",
        "ojosproject",
        "Pipenv",
        "rusqlite",
        "Tauri"
    ],
    "indentRainbow.indentSetter": {
        "markdown": 2
    },
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "${workspaceFolder:iris}/src-tauri/iris.db",
            "name": "Iris DB"
        }
    ],
    "sqltools.useNodeRuntime": true
}